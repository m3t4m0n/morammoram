<%= form_for(@team) do |f| %>

  <div class="field">
    request.path
    <%= f.hidden_field :contest_id, value: params[:cid] %>
    <%= f.hidden_field :grade, value: 2 %>
    <%= f.hidden_field :user_id, value: current_user.id%>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
